package dec4;

public class OperatorsDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Unary
		int a = 10;
		int b = 20;
		a++;
		System.out.println(a);
//		System.out.println(a);
		
		//Arithmetic
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);

	}

}
